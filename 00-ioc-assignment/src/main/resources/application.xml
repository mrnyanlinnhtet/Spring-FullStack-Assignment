<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">


	<bean id="message" class="java.lang.String">
		<constructor-arg
			value="Hello IoC Container on Servlet" />
	</bean>

	<bean id="courseModel" class="com.ultron.model.impl.CourseModelImpl" />
	<bean id="openClassModel" class="com.ultron.model.impl.OpenClassImpl" />
	<bean id="registrationModel"
		class="com.ultron.model.impl.RegistrationImpl" />
</beans>
